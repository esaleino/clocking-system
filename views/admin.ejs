<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%- include('header')-%> <%- include('navbar')-%>
    <link rel="stylesheet" href="../stylesheets/dashboard.css" />
    <script type="module" src="/javascripts/admin.js"></script>
    <script>
      var myUsername = '<%-JSON.stringify(loggedinUser)%>';
      var currentSection = '';
    </script>
  </head>

  <body onload="pageLoadAdmin()">
    <%- include('sidebarAdmin')-%>
    <main role="main" class="col-md-9 col-sm-6 ml-sm-auto col-lg-10 px-4">
      <div
        class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom"
      >
        <h1 class="h2">Dashboard</h1>
        <div class="btn-toolbar mb-2 mb-md-0"></div>
      </div>
      <h2 id="sectionTitle">This section will hold data</h2>
      <table
        class="table table-striped table-bordered w-100"
        id="table"
        style="width: 100%"
      >
        <thead id="tableHead"></thead>
        <tbody id="tableBody"></tbody>
      </table>
      <div id="form"></div>
    </main>
  </body>
</html>
